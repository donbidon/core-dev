{
  "name": "donbidon/core-dev",
  "type": "library",
  "description": "Small library implementing registry, logging and events",
  "homepage": "https://github.com/donbidon/core-dev",
  "keywords": ["php", "core", "registry", "events", "logging"],
  "license": "MIT",
  "authors": [
    {
      "name": "donbidon",
      "homepage": "http://donbidon.rf.gd/"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "squizlabs/php_codesniffer": "3.*",
    "slevomat/coding-standard": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "donbidon\\Core\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "donbidon\\Core\\":  ["tests", "tests/src"]
    }
  }
}
